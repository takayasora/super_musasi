{
  "name": "Super Musasi",
  "version": "3.0.0",
  "manifest_version": 2,
  "description": "合宿免許アプリMusasiに対して拡張機能を実装しています。",
  "content_scripts": [{
    "matches": ["https://www.musasi.jp/question/*"],
    "js": [
      "content_question.js"
    ]
  },{
    "matches": ["https://www.musasi.jp/explanation/*"],
    "js": [
      "content_explanation.js"
    ]
  }]  
}
